<Window x:Class="MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Mers et Océans"
        Width="{DynamicResource {x:Static SystemParameters.PrimaryScreenWidthKey}}"
        Height="{DynamicResource {x:Static SystemParameters.PrimaryScreenHeightKey}}"
        HorizontalAlignment="Center"
        VerticalAlignment="Center"
        Icon="Resources/Icone_Mers.ico"
        Loaded="Window_Loaded"
        WindowStartupLocation="CenterScreen"
        WindowState="Maximized">
    <Window.Resources>
        <Storyboard x:Key="Label_points">
            <ColorAnimationUsingKeyFrames Storyboard.TargetName="Points" Storyboard.TargetProperty="(Panel.Background).(SolidColorBrush.Color)">
                <EasingColorKeyFrame KeyTime="0" Value="#FFE8FF00" />
                <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FFFF4600" />
                <EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FFE8FF00" />
            </ColorAnimationUsingKeyFrames>
        </Storyboard>
    </Window.Resources>
    <Window.Triggers>
        <EventTrigger RoutedEvent="ButtonBase.Click" SourceName="vérifier">
            <BeginStoryboard Storyboard="{StaticResource Label_points}" />
        </EventTrigger>
        <EventTrigger RoutedEvent="FrameworkElement.Loaded" />
    </Window.Triggers>
